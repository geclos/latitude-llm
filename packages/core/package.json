{
  "name": "@latitude-data/core",
  "version": "0.0.1",
  "description": "Core services used across the latitude LLM stack",
  "type": "module",
  "exports": {
    "./browser": "./src/browser.ts",
    "./cache": "./src/cache/index.ts",
    "./client": "./src/client/index.ts",
    "./data-access": "./src/data-access/index.ts",
    "./events/*": "./src/events/*.ts",
    "./lib/*": "./src/lib/*.ts",
    "./queues": "./src/queues/index.ts",
    "./redis": "./src/redis/index.ts",
    "./repositories": "./src/repositories/index.ts",
    "./repositories/*": "./src/repositories/*.ts",
    "./schema": "./src/schema/index.ts",
    "./schema/*": "./src/schema/*.ts",
    "./services/*": "./src/services/*.ts",
    "./websockets/*": "./src/websockets/*.ts",
    "./factories": "./src/tests/factories/index.ts",
    "./test": "./src/tests/useTestDatabase.ts"
  },
  "scripts": {
    "db:generate": "drizzle-kit generate",
    "db:generate:custom": "pnpm run db:generate --custom",
    "db:migrate": "drizzle-kit migrate --config=./drizzle.config.ts",
    "db:migrate:test": "NODE_ENV=test pnpm run db:migrate",
    "db:drop": "drizzle-kit drop",
    "db:check": "drizzle-kit check",
    "db:studio": "drizzle-kit studio --port 3003",
    "lint": "eslint src",
    "tc": "tsc --noEmit",
    "test": "pnpm run db:migrate:test && vitest run --pool=forks",
    "test:watch": "vitest",
    "prettier": "prettier --write src/**/*.ts"
  },
  "devDependencies": {
    "@ai-sdk/anthropic": "^0.0.48",
    "@ai-sdk/azure": "^0.0.32",
    "@ai-sdk/google": "^0.0.46",
    "@ai-sdk/mistral": "^0.0.38",
    "@ai-sdk/openai": "^0.0.54",
    "@aws-sdk/client-s3": "^3.645.0",
    "@aws-sdk/s3-request-presigner": "^3.645.0",
    "@faker-js/faker": "^8.4.1",
    "@latitude-data/compiler": "workspace:^",
    "@latitude-data/env": "workspace:*",
    "@latitude-data/eslint-config": "workspace:*",
    "@latitude-data/typescript-config": "workspace:*",
    "@sindresorhus/slugify": "^2.2.1",
    "@t3-oss/env-core": "^0.11.1",
    "@types/json-schema": "^7.0.15",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.5.0",
    "@types/pg": "^8.11.6",
    "@types/uuid": "^10.0.0",
    "ai": "^3.2.42",
    "argon2": "^0.41.0",
    "csv-parse": "^5.5.6",
    "drizzle-kit": "^0.22.8",
    "drizzle-orm": "^0.33.0",
    "eslint": "8",
    "eslint-plugin-drizzle": "^0.2.3",
    "flydrive": "^1.1.0",
    "jose": "^5.8.0",
    "json-schema": "^0.4.0",
    "lodash-es": "^4.17.21",
    "pg": "^8.12.0",
    "pg-transactional-tests": "^1.0.9",
    "posthog-node": "^4.2.0",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "socket.io-client": "^4.7.5",
    "supertest": "^7.0.0",
    "svelte": "^4.2.19",
    "uuid": "^10.0.0",
    "vitest": "^2.0.3",
    "vue": "^3.4.38",
    "zod": "^3.23.8"
  },
  "peerDependencies": {
    "@ai-sdk/anthropic": "^0.0.48",
