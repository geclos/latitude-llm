    "@aws-sdk/s3-request-presigner": "^3.645.0",
    "@latitude-data/compiler": "workspace:^",
    "@latitude-data/env": "workspace:^",
    "@latitude-data/jobs": "workspace:^",
    "@latitude-data/mailers": "workspace:^",
