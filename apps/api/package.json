    "find": "^0.3.0",
    "fs-extra": "^11.2.0",
    "jet-paths": "^1.0.6",
    "prettier": "^3.2.5",
    "supertest": "^7.0.0",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.16.0",
