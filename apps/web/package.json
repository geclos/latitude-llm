  "packageManager": "pnpm@8.6.3",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^8.14.0",
    "@t3-oss/env-nextjs": "^0.10.1",
    "drizzle-orm": "^0.31.2",
    "next": "15.0.0-rc.0",
    "pg": "^8.12.0",
    "react": "19.0.0-rc-f994737d14-20240522",
    "react-dom": "19.0.0-rc-f994737d14-20240522",
    "vitest": "^1.6.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@latitude-data/typescript-config": "workspace:*",
    "@types/node": "^20",
    "@types/pg": "^8.11.6",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.22.8",
    "eslint": "^8",
