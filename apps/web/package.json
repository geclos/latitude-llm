{
  "name": "@latitude-data/web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "tc": "tsc --noEmit",
    "test": "vitest --run",
    "test:watch": "vitest",
    "prettier": "prettier --write \"**/*.{ts,tsx,md}\"",
