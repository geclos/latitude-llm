{
  "name": "@latitude-data/gateway",
  "version": "1.0.0",
  "type": "module",
  "description": "Gateway for Latitude Data",
  "private": true,
  "scripts": {
    "build": "tsup --config tsup.config.ts",
    "dev": "tsx watch src/server",
    "dev:debug": "tsx watch --inspect-brk src/server",
    "lint": "eslint src/",
    "prettier": "prettier --write \"**/*.{ts,tsx,md}\"",
    "tc": "tsc --noEmit",
    "test": "vitest --run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@hono/node-server": "^1.12.0",
    "@hono/zod-validator": "^0.2.2",
    "@latitude-data/core": "workspace:^",
    "@latitude-data/env": "workspace:^",
    "@latitude-data/jobs": "workspace:^",
    "@t3-oss/env-core": "^0.10.1",
    "hono": "^4.5.3",
    "jet-paths": "^1.0.6",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@latitude-data/eslint-config": "workspace:^",
    "@latitude-data/typescript-config": "workspace:^",
    "@types/uuid": "^10.0.0",
    "eslint": "8",
    "tsup": "^8.2.4",
    "tsx": "^4.16.2",
    "typescript": "^5.5.4",
    "vitest": "^2.0.4"
  }
}
